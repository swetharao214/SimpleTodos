#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/coding-practices/simpleTodos/node_modules/.pnpm/eslint@7.31.0/node_modules/eslint/bin/eslint.js/node_modules:/home/workspace/reactjs/coding-practices/simpleTodos/node_modules/.pnpm/eslint@7.31.0/node_modules/eslint/bin/node_modules:/home/workspace/reactjs/coding-practices/simpleTodos/node_modules/.pnpm/eslint@7.31.0/node_modules/eslint/node_modules:/home/workspace/reactjs/coding-practices/simpleTodos/node_modules/.pnpm/eslint@7.31.0/node_modules:/home/workspace/reactjs/coding-practices/simpleTodos/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/simpleTodos/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/simpleTodos/node_modules/.pnpm/eslint-config-react-app@6.0.0_2143f50b1cc99cd71765356afca10f0f/node_modules/eslint-config-react-app/node_modules:/home/workspace/reactjs/coding-practices/simpleTodos/node_modules/.pnpm/eslint-config-react-app@6.0.0_2143f50b1cc99cd71765356afca10f0f/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@7.31.0/node_modules/eslint/bin/eslint.js" "$@"
else 
  exec node  "$basedir/../../../../../eslint@7.31.0/node_modules/eslint/bin/eslint.js" "$@"
fi
